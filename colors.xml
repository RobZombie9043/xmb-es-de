<theme>
	<colorScheme name="20th-anniversary">
        <variables>
			<folderName>20th-anniversary</folderName>
			<extName>png</extName>
			<blurBG>false</blurBG>
			<bgCropPos>0.85 0.5</bgCropPos>
			<gamelistImagePath>./_inc/images/PS20th.webp</gamelistImagePath>
			<gamelistDefaultImagePath>./_inc/images/PS20th.webp</gamelistDefaultImagePath>
			<gamelistImagePathVisible>true</gamelistImagePathVisible>
			<gamelistImageTypeVisible>false</gamelistImageTypeVisible>
			<bgLogoVisible>false</bgLogoVisible>
			<systemUnfocusedItemOpacity>0.5</systemUnfocusedItemOpacity>
			<systemUnfocusedItemDimming>1</systemUnfocusedItemDimming>
			<gamelistUnfocusedItemOpacity>0.5</gamelistUnfocusedItemOpacity>
			<gamelistUnfocusedItemBrightness>0</gamelistUnfocusedItemBrightness>
			<scrollFadeIn>false</scrollFadeIn>
			<textColor>FFFFFF</textColor>
			<statusBGColor>80808026</statusBGColor>
		</variables>
    </colorScheme>
	<colorScheme name="famicom">
        <variables>
			<folderName>gold</folderName>
			<extName>png</extName>
			<blurBG>false</blurBG>
			<bgCropPos>0.5 0.5</bgCropPos>
			<gamelistImagePath>./_inc/images/famicom-red.png</gamelistImagePath>
			<gamelistDefaultImagePath>./_inc/images/famicom-red.png</gamelistDefaultImagePath>
			<gamelistImagePathVisible>true</gamelistImagePathVisible>
			<gamelistImageTypeVisible>false</gamelistImageTypeVisible>
			<bgLogoVisible>false</bgLogoVisible>
			<systemUnfocusedItemOpacity>0.7</systemUnfocusedItemOpacity>
			<systemUnfocusedItemDimming>1</systemUnfocusedItemDimming>
			<gamelistUnfocusedItemOpacity>0.7</gamelistUnfocusedItemOpacity>
			<gamelistUnfocusedItemBrightness>0</gamelistUnfocusedItemBrightness>
			<scrollFadeIn>false</scrollFadeIn>
			<textColor>c1a05b</textColor>
			<statusBGColor>2f090eE6</statusBGColor>
		</variables>
    </colorScheme>
	<colorScheme name="background">
        <variables>
			<folderName>monochrome</folderName>
			<extName>png</extName>
			<blurBG>true</blurBG>
			<bgCropPos>0.5 0.5</bgCropPos>
			<gamelistImagePath>./_inc/systems/modern/${system.theme}.webp</gamelistImagePath>
			<gamelistDefaultImagePath>./_inc/systems/modern/_default.webp</gamelistDefaultImagePath>
			<gamelistImagePathVisible>false</gamelistImagePathVisible>
			<gamelistImageTypeVisible>true</gamelistImageTypeVisible>
			<bgLogoVisible>false</bgLogoVisible>
			<systemUnfocusedItemOpacity>1</systemUnfocusedItemOpacity>
			<systemUnfocusedItemDimming>0.75</systemUnfocusedItemDimming>
			<gamelistUnfocusedItemOpacity>1</gamelistUnfocusedItemOpacity>
			<gamelistUnfocusedItemBrightness>-1</gamelistUnfocusedItemBrightness>
			<scrollFadeIn>true</scrollFadeIn>
			<textColor>FFFFFF</textColor>
			<statusBGColor>80808026</statusBGColor>
		</variables>
    </colorScheme>
